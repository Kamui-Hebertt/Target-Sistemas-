include <stdio.h>

int main() {
    int INDICE = 13, SOMA = 0, K = 0;

    while (K < INDICE) {
        K = K + 1;
        SOMA = SOMA + K;
    }

    printf("O valor da variável SOMA é: %d\n", SOMA);

    return 0;


}

//Ao executar esse código, a saída será: "O valor da variável SOMA é: 91",
// confirmando que a variável SOMA terá o valor 91 ao final do processamento.